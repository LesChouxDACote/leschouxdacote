{
  "search.exclude": {
    "yarn.lock": true
  },
  "files.exclude": {
    "node_modules/": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[yaml]": {
    "editor.formatOnSave": true
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "cSpell.words": [
    "coolify",
    "Coolify",
    "firestore",
    "firstname",
    "Geoloc",
    "ilieff",
    "lastname",
    "leschouxdacote",
    "Mapbox",
    "signin",
    "signout",
    "siret",
    "Vaultwarden"
  ]
}
